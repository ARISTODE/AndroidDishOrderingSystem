<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hyz.wind_dishordering"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="23" />

    <application
        android:name="com.hyz.wind_dishordering.Dishdata"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.hyz.wind_dishordering.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hyz.wind_dishordering.CheckoutSubsystem.CheckoutPageActivity"
            android:launchMode="singleTask" />
        <activity
            android:name="com.hyz.wind_dishordering.ChefSubsystem.ChefActivity"
            android:launchMode="singleTask" />
        <activity android:name="com.hyz.wind_dishordering.AdminSubsystem.AdminActivity" />
        <activity android:name="com.hyz.wind_dishordering.AdminSubsystem.SetDiscount" />
    </application>

</manifest>